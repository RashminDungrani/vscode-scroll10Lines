{
	"name": "scroll10lines",
	"displayName": "scroll10Lines",
	"description": "Scroll 10 lines Up and Down without moving cursor",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:scroll10lines.scroll10LinesUp",
		"onCommand:scroll10lines.scroll10LinesDown"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "scroll10lines.scroll10LinesUp",
				"title": "Scroll 10 Lines of Up"
			},
			{
				"command": "scroll10lines.scroll10LinesDown",
				"title": "Scroll 10 Lines of Down"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"ts-loader": "^7.0.5",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11"
	},
	"dependencies": {
		"url-parse": "^1.5.9"
	}
}
